        {
          "name": "stdout",
          "output_type": "stream",
          "text": [
            "4500  mb\n",
            "9000  mb\n",
            "360000  mb\n"
          ]
        }
      ],
      "source": [
        "def red(x):\n",
        "    return 100*x\n",
        "\n",
        "print(red(45), \" mb\")\n",
        "print(red(90), \" mb\")\n",
        "print(red(3600), \" mb\")\n",
        "\n"
      ]
    },
